<controls:ChildWindow x:Class="Virtuoso.Core.Controls.AuthDistributionPopup"
                      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
                      xmlns:VirtuosoCore="clr-namespace:Virtuoso.Core;assembly=Virtuoso.Core"
                      xmlns:VirtuosoCoreView="clr-namespace:Virtuoso.Core.View;assembly=Virtuoso.Core"
                      Style="{StaticResource ChildWindowStyle}"
                      Title="Authorization Distribution"
                      VirtuosoCore:BindableDialogResult.DialogResult="{Binding DialogResult, Mode=TwoWay}"
                      Closing="ChildWindow_Closing">
    <Grid x:Name="LayoutRoot"
          Margin="2">
        <VirtuosoCoreView:AuthDistributionView />
    </Grid>
</controls:ChildWindow>